{
  "name": "nz-press",
  "version": "0.3.4",
  "description": "An angular site generator inspiring by ng-zorro and vuepress",
  "main": "index.js",
  "bin": {
    "nzpress": "bin/nzpress"
  },
  "scripts": {
    "start": "node ./bin/nzpress dev ./docs",
    "init": "node ./bin/nzpress init ./docs",
    "build": "node ./bin/nzpress build ./docs --base-href=./",
    "build-test": "bash ./build-test.sh",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s --pkg package.json",
    "doc:commit": "rm -rf ../nzpress-doc/* && cp -r ./docs/.nzpress/dist/* ../nzpress-doc/ && cd ../nzpress-doc/ && git add . && git commit -m 'update: docs' && git push",
    "release": "node ./scripts/publish/publish.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wenqi73/nz-press"
  },
  "keywords": [
    "angular",
    "ng-zorro-antd",
    "site-generator"
  ],
  "author": "wenqi73",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/wenqi73/nz-press/issues"
  },
  "homepage": "https://github.com/wenqi73/nz-press#readme",
  "dependencies": {
    "@angular-devkit/build-angular": "~0.7.5",
    "@angular/animations": "^6.1.6",
    "@angular/cli": "^6.1.6",
    "@angular/common": "^6.1.6",
    "@angular/compiler": "^6.1.6",
    "@angular/compiler-cli": "^6.1.6",
    "@angular/core": "^6.1.6",
    "@angular/forms": "^6.1.6",
    "@angular/http": "^6.1.6",
    "@angular/language-service": "^6.1.6",
    "@angular/platform-browser": "^6.1.6",
    "@angular/platform-browser-dynamic": "^6.1.6",
    "@angular/router": "^6.1.6",
    "@types/node": "~8.9.4",
    "chalk": "^2.4.1",
    "commander": "^2.19.0",
    "core-js": "^2.5.4",
    "fs-extra": "^7.0.1",
    "marked": "^1.1.1",
    "ng-zorro-antd": "^1.4.0",
    "node-prismjs": "^0.1.2",
    "remark": "^9.0.0",
    "rxjs": "^6.3.0",
    "ts-node": "~5.0.1",
    "typescript": "^2.7.2",
    "yaml-front-matter": "^4.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "codelyzer": "~4.2.1",
    "readline-sync": "^1.4.9",
    "tslint": "~5.9.1"
  },
  "engines": {
    "node": ">=8"
  }
}
